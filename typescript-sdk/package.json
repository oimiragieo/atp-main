{
  "name": "@atp-project/atp-sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for ATP Router Protocol",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "prepublishOnly": "npm run build",
    "docs": "typedoc src/index.ts"
  },
  "keywords": [
    "atp",
    "router",
    "websocket",
    "typescript",
    "sdk"
  ],
  "author": "ATP Project",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.13",
    "@types/ws": "^8.5.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.0.0",
    "typedoc": "^0.25.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "ws": "^8.0.0"
  },
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/atp-project/atp-main.git",
    "directory": "typescript-sdk"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
